extends layout

block content

  h1 Category: #{category.name}
  h3 #{category.description}
  
  div(style='margin-left:20px;margin-top:20px')

    h4 Clubs
    
    dl
      each item in category_items
        dt 
          a(href=item.url) #{item.name}

      else
        p This category has no items